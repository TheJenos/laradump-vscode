{
  "name": "laradump",
  "displayName": "Laradump",
  "description": "Laradump helps to debugging on laravel",
  "publisher": "laradump",
  "icon": "logo.png",
  "repository": "https://github.com/TheJenos/laradump-vscode",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laradump.startServer",
        "title": "Start laradump server"
      },
      {
        "command": "laradump.stopServer",
        "title": "Stop laradump server"
      }
    ]
  },
  "scripts": {
    "tailwind": "NODE_ENV=production npx tailwindcss-cli@latest build ./style/main.css --minify -c ./tailwind.config.js -o ./media/tailwind.css",
    "build": "tailwind-watch",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "laradump-server": "file:lib\\laradump-server"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "tailwind-watch": "^0.0.6",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
